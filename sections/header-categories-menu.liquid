{%- liquid
  assign se_stts = section.settings
  assign se_blocks = section.blocks 
  assign admin_sp = request.design_mode
  assign h__bgimg = se_stts.h__bgimg
  assign h_transparent = se_stts.transparent_header
  if request.page_type != 'index' 
    assign h_transparent = false 
  endif
  assign space_transparent = se_stts.space_transparent_header -%}

{%- style -%}
  {%- assign opnav  = se_stts.opnav | divided_by: 100.0 -%}
  .sr4-header__wrapper {
    --h-text-color      : {{se_stts.clnav}};
    --h-text-color-rgb  : {{ se_stts.clnav | color_to_rgb | remove: 'rgba(' | remove: 'rgb(' | remove: ')' }};
    --h-text-color-hover: {{se_stts.clnav_hover}};
    --h-bg-color        : {{ se_stts.bgnav | color_modify: 'alpha', opnav }};
    background-color: var(--h-bg-color);
  }
  .sr4-section-header__bot {
    background-color: {{ se_stts.bgnav2 }};
    color: {{ se_stts.clnav2 }};
  }
  .sr4-count-box {
    --h-count-bgcolor: {{se_stts.bg_hc}};
    --h-count-color: {{se_stts.cl_hc}};
  }

  {%- if h__bgimg != blank %}
  .sr4-header__bgimg {
      background-size: cover;
      background-repeat: no-repeat;
  }
  {%- endif -%}

  {%- if h_transparent -%}

    .sr4-section-header,#shopify-section-top-bar { --h-space-tr: {% if space_transparent %}30px{% else %}0px{% endif %} }

    {%- assign opnavtr  = se_stts.opnavtr | divided_by: 100.0 -%}
    .sr4-header__wrapper {
      --h-text-color      : {{se_stts.clnavtr}};
      --h-text-color-rgb  : {{ se_stts.clnavtr | color_to_rgb | remove: 'rgba(' | remove: 'rgb(' | remove: ')' }};
      --h-text-color-hover: {{se_stts.clnavtr_hover}};
      --h-bg-color        : {{ se_stts.bgnavtr | color_modify: 'alpha', opnavtr }};
    }
    .sr4-section-header {
        margin-top: var(--h-space-tr);
        margin-bottom: calc(-1 * (var(--header-height) + var(--h-space-tr)));
        position: relative;
        top: 0;
        z-index: 460;
    }
    .is--header-transparent .sr4-header__design2 .sr4-search-header__form{
        --bg-cl-form:{{se_stts.bgseatr}}!important;
        --br-cl-form:{{se_stts.bgseatr | color_darken: 15}};
    }

    {%- if space_transparent -%}
      .is--topbar-transparent.is--header-transparent #shopify-section-top-bar {
        margin-top: var(--h-space-tr);
      }
      #sr4-hsticky__sentinel {
        bottom:calc(-1 * var(--h-space-tr));
      }
    {%- endif -%}

  {%- endif -%}

  {%- if se_stts.sticky_header -%}
  
    {%- unless se_stts.scroll_header -%}
    .sr4-hsticky__ready .sr4-section-header {
        position: sticky;      
        top: 0;
        z-index: 460;
    }
    .is-header--stuck .sr4-section-header {
      -webkit-box-shadow: 0 1px 3px rgb(0 0 0 / 10%);
      box-shadow: 0 1px 3px rgb(0 0 0 / 10%);
    }
    {%- endunless -%}
    
    {%- assign opnavst  = se_stts.opnavst | divided_by: 100.0 -%}
    .is-header--stuck .sr4-header__wrapper {
      --h-text-color      : {{se_stts.clnavst}};
      --h-text-color-rgb  : {{ se_stts.clnavst | color_to_rgb | remove: 'rgba(' | remove: 'rgb(' | remove: ')' }};
      --h-text-color-hover: {{se_stts.clnavst_hover}};
      --h-bg-color        : {{ se_stts.bgnavst | color_modify: 'alpha', opnavst }};
    }
    .is-header--stuck .header__sticky-logo {
      display:block !important
    }
    .is-header--stuck .header__normal-logo,
    .is-header--stuck .header__mobile-logo {
      display:none !important
    }
    .is-header--stuck .sr4-header__design2 .sr4-search-header__form{
        --bg-cl-form:{{se_stts.bgseavst}};
        --br-cl-form:{{se_stts.bgseavst | color_darken: 15}};
    }
  {%- endif -%}
 
  .sr4-section-header [data-header-height] {
      min-height: {{se_stts.h_navmb}}px;    
  }
  .sr4-section-header [data-header-height2] {
      min-height: {{se_stts.height2}}px;    
  }
  .sr4-header__logo img {
    padding-top: 5px;
    padding-bottom: 5px;
    transform: translateZ(0);
    max-height: inherit;
    height: auto;
    width: 100%;
    max-width: 100%;
  }
  .sr4-header__logo img[src*=".svg"] {
    height: 100%;
    perspective: 800px;
    -webkit-perspective: 800px;
    backface-visibility: hidden;
    -webkit-backface-visibility: hidden;
  }
  .sr4-site-nav__icons .sr4-site-nav__icon {
      padding: 0 6px;
      display: inline-block;
      line-height: 1;
  }
  .sr4-site-nav__icons svg.sr4-icon {
      color: var(--h-text-color);
      line-height: 1;
      vertical-align: middle;
      transition: color 0.2s ease-in-out;
      width: 22px;
      height: 22px;
  }
  .sr4-site-nav__icons.sr4-use__kalles svg.sr4-icon--account {
      width: 24px;
      height: 24px;
  }
  .sr4-site-nav__icons.sr4-use__line svg.sr4-icon {
    width: 25px;
    height: 25px;
  }
  .sr4-site-nav__icon>a:hover svg.sr4-icon {
      color: var(--h-text-color-hover);
  }
  .sr4-site-nav__icon a { 
    display: inline-block;
    line-height: 1;
  }
  .sr4-site-nav__cart >a,.sr4-push-menu-btn {color: var(--h-text-color)}
  .sr4-site-nav__cart >a:hover {color: var(--h-text-color-hover)}
  @media (min-width: 768px) {
    .sr4-site-nav__icons .sr4-site-nav__icon {
        padding: 0 8px;
    }
  }
  @media (min-width: 1025px) {
      {%- if se_stts.border -%}
      .sr4-section-header__mid {
        border-bottom: 1px solid rgba(var(--h-text-color-rgb), 0.15);
       }
      {%- endif -%}
      .sr4-section-header [data-header-height] {
         min-height: {{se_stts.height}}px;    
      }
      {%- if se_stts.full_header -%}
      .sr4-announcement-bar >.sr4-container, .sr4-top-bar >.sr4-container,
      .sr4-header__wrapper .sr4-section-header__mid >.sr4-container, 
      .sr4-header__wrapper .sr4-section-header__bot>.sr4-container {
          max-width: 100%;
      }

      {%- else -%}
         {%- if space_transparent and h_transparent -%}
          .is--topbar-transparent.is--header-transparent #shopify-section-top-bar,
          html:not(.is-header--stuck) .sr4-section-header {
            max-width: 1170px;
            margin-right: auto;
            margin-left: auto;
          }
        {%- endif -%}
        .sr4-header__wrapper>.sr4-container {
          padding-right: 20px;
          padding-left: 20px;
        }
      {%- endif -%}
      .sr4-nav__ul {
          margin: 0;padding:0
      }
    .sr4-nav__ul>li> a {
        color: {{se_stts.clnav2}};
        font-weight: 400;
        font-size: 14px;
        letter-spacing: 0;
        padding: 5px 17.5px;
        text-transform: none;
        font-family: var(--font-family-{{se_stts.fm_nav}});
        font-weight: {{se_stts.fw_nav}};
        font-size: {{se_stts.fs_nav}}px;
        {%- if se_stts.ls_nav != 0 %}letter-spacing: {{se_stts.ls_nav}}px;{% endif -%}
    }
    #sr4-nav-categories { color : var(--text-color)}
    .sr4-section-header__bot .sr4-h-cat__html a { color: {{se_stts.clnav2}}; }
    .sr4-nav__ul>li> a:hover,
    .sr4-section-header__bot .sr4-h-cat__html a:hover { opacity:.8; color: {{se_stts.clnav2 | color_lighten: 15}} !important}
    .sr4-section-header__bot .sr4-h-cat {
      background-color: {{se_stts.bgcategory }};
      transition: color .25s ease, background-color .25s ease, border-color .25s ease, box-shadow .25s ease, opacity .25s ease;
    }
    .sr4-section-header__bot .sr4-h-cat:hover {
      background-color: {{se_stts.bgcategory | color_darken: 15}};
    }
    .sr4-section-header__bot .sr4-h-cat >h5 { 
      color: {{se_stts.clnav2}};
      min-height: {{se_stts.height2}}px;
      font-size: 14px;    
      width: 230px;
      padding:0px 15px;
      cursor: pointer;
      transition: all .2s ease 0s;
     }
     .sr4-header__design2 .sr4-section-header__bot .sr4-h-cat >h5{padding:0px;}
     .sr4-section-header__bot .sr4-h-cat >h5 svg {
        margin-right: 5px;
        width:20px;
        height:20px
    }
    
    .sr4-h-cat >svg.sr4-icon-select-arrow {
        margin-right: 7px;
    }
    .sr4-nav__ul .sr4-icon-select-arrow {
        position: static;
        width: 8px;
        margin-left: 4px;
        height: 8px;
        opacity: .8;
    }
    .sr4-h-cat__html svg {
        width: 16px;
        height: 16px;
        vertical-align: middle;
        display: inline-block;
    }
    .sr4-h-cat__html .ml__15 {
        margin-left: 15px;
    }
    .sr4-section-header__bot .sr4-h-cat__html {
      font-size: 12px
    }
    .sr4-section-header__bot .sr4-h-cat__html svg {
      width:14px;height:14px;
    }
    .sr4-layout-layout_categories .sr4-site-nav__icon.sr4-site-nav__search {
      display: none;
    }
    {%- if se_stts.enable_active %}
    .sr4-nav__ul>li.is--nav__active> a {
      color: var(--h-text-color-hover) !important;
      transition: none;
    }
    {%- endif -%}
  }
{%- endstyle -%}

<div data-header-options='{ "isTransparent": {{h_transparent}},"isSticky": {{se_stts.sticky_header}},"hideScroldown": {{se_stts.scroll_header}} }' class="sr4-header__wrapper sr4-header__design{{se_stts.header_design}} sr4-layout-layout_categories{% if h__bgimg != blank %} lazyloadsr4 sr4-header__bgimg" data-bgset="{{ h__bgimg | image_url: width: 1 }}" data-ratio="{{h__bgimg.aspect_ratio}}" data-sizes="auto"{% else %}"{% endif %}>
  <div class="sr4-section-header__mid sr4-pr">
    <div class="sr4-container">
      <div data-header-height class="sr4-row sr4-gx-15 sr4-gx-md-30 sr4-align-items-center">
          <div class="sr4-col-md-4 sr4-col-3 sr4-d-lg-none sr4-col-item">{%- render 'push_menu' -%}</div>
          <div class="sr4-col-lg-3 sr4-col-md-4 sr4-col-6 sr4-text-center sr4-text-lg-start sr4-col-item">{%- render 't4s_logo', tag:'div', isTransparent:h_transparent-%}</div>
          <div data-predictive-search data-sid="search-hidden" class="sr4-search-header__form-wrap sr4-d-none sr4-d-lg-block sr4-col-6 sr4-col-item">

            {%- style -%}
                .sr4-search-header__form {
                    padding: 0;
                    border: 1px solid rgba(var(--h-text-color-rgb), 0.15);
                    border-radius: var(--btn-radius);
                    padding: 2px;
                    max-width: {{se_stts.frm_sea_mw}}px;
                    margin: 0 auto;
                    --bg-cl-form: {{se_stts.bgsea}};
                    background-color:var(--bg-cl-form);
                }
                .sr4-header__design2 .sr4-search-header__form{
                    --br-cl-form:{{se_stts.bgsea | color_darken: 15 }};
                    border: 1px solid var(--br-cl-form);
                }

                .sr4-header__design2 .sr4-search-header__form,
                .sr4-header__design2 .sr4-search-header__form .sr4-search-header__submit{border-radius:0px;}
                .sr4-header__design2 .sr4-search-header__form .sr4-search-header__submit{min-width:56px;background-color:transparent;}
                
                .sr4-search-header__submit-icon svg{
                    width:18px;height:18px;
                    transition: .35s cubic-bezier(.25, .1, .25, 1);
                    color: var(--h-text-color);
                }
                .sr4-header__design2 .sr4-search-header__submit-text{display:none}
                .sr4-header__design2 .sr4-search-header__submit-icon{display:block!important;}
                .sr4-search-header__input {
                    background-color: transparent;
                    padding: 0 15px;
                    height: 40px;
                    border: 0;
                    width: 100%;
                    line-height: 18px;
                    color: var(--h-text-color);
                    border-radius: var(--btn-radius);
                        font-size: 13px;
                }
                .sr4-h-cat-br__true:after {
                    position: absolute;
                    content: '';
                    width: 1px;
                    height: 20px;
                    background: rgba(var(--h-text-color-rgb), 0.15);
                    top: 50%;
                    transform: translateY(-50%);
                    right: 0;
                }

              {%- assign clsea_lightness  = se_stts.clsea | color_extract: 'lightness' -%}
              .sr4-search-header__submit {
                  --h-check: {{clsea_lightness}};
                  --h-btn-color : {% if clsea_lightness < 85 %}#fff{% else %}#222{% endif %};
                  --h-btn-bg-color : {{se_stts.clsea}};
                  --h-btn-bg-color-hover : {{se_stts.clsea | color_darken: 15}};
                  margin: 0;
                  min-width: 130px;
                  font-weight: 600;
                  border-radius: var(--btn-radius);
                  background-color: var(--h-btn-bg-color);
                  color: var(--h-btn-color);
                  font-size: 14px;
                  transition: color .25s ease, background-color .25s ease, border-color .25s ease, box-shadow .25s ease, opacity .25s ease;
              }
            {%- if h_transparent -%}
              {%- assign clsea_lightness  = se_stts.clseatr | color_extract: 'lightness' -%}
              .sr4-search-header__submit {
                  --h-btn-color : {% if clsea_lightness < 85 %}#fff{% else %}#222{% endif %};
                  --h-btn-bg-color : {{se_stts.clseatr}};
                  --h-btn-bg-color-hover : {{se_stts.clseatr | color_darken: 15}};
              }
            {%- endif -%}

              {%- assign clsea_lightness  = se_stts.clseast | color_extract: 'lightness' -%}
                .is-header--stuck .sr4-search-header__submit {
                        --h-btn-color : {% if clsea_lightness < 85 %}#fff{% else %}#222{% endif %};
                        --h-btn-bg-color : {{se_stts.clseast}};
                        --h-btn-bg-color-hover : {{se_stts.clseast | color_darken: 15}};
                }

              .sr4-search-header__submit:hover {
                 background-color: var(--h-btn-bg-color-hover);
                 color: var(--h-btn-color);
              }
              .sr4-site-nav__icons .sr4-search-header__submit svg.sr4-icon {
                    color: rgba(var(--h-text-color-rgb), 0.15);
                    width: 15px;
                    height: 15px;
              }
              .sr4-search-header__type select {
                  border: 0;
                  max-width: 100%;
                  padding: 0 30px 0 15px;
                  -webkit-appearance: none;
                  -moz-appearance: none;
                  appearance: none;
                  font-size: 14px;
                  display: inline-block;
                  background-color: transparent;
                  box-shadow: none;
                  color: var(--h-text-color);
                  border-radius: var(--btn-radius);
              }
              .sr4-search-header__type .sr4-icon-select-arrow { color: rgba(var(--h-text-color-rgb), 1); }
              .sr4-search-header_border {
                  height: 18px;
                  background-color: rgba(var(--h-text-color-rgb), 0.15);
                  width: 1.5px;
              }
              .sr4-frm-search__results {
                    position: absolute;
                    top: 100%;
                    right: 0;
                    left: 0;
                    z-index: 1000;
                    width: auto;
                    height: auto;
                    background-color: var(--sr4-light-color);
                    margin-top: 5px;
                    opacity: 0;
                    visibility: hidden;
                    pointer-events: none;
                    transition: all .1s ease-in-out;
                    max-width: 600px;
                    margin: 0 auto;
                    box-shadow: 0 1px 5px 2px rgba(var(--border-color-rgb),.3);
              }
              .calc-pos-submenu .sr4-search-header__form-wrap:hover .sr4-frm-search__results {
                  opacity: 1;
                  visibility: visible;
                  pointer-events: auto;
              }
              .sr4-frm-search__content { 
                height:auto;
                overflow: auto;
                overflow-x: hidden;
                -webkit-overflow-scrolling: touch;
                padding: 20px;
              }
              .sr4-frm-search__content .sr4-widget_img_pr {
                  min-width: 95px;
                  max-width: 95px;
                  max-height: 120px;
              }
              .sr4-frm-search__content .sr4-widget_img_pr>a {
                height: 100%;
              }
              .sr4-frm-search__content .sr4-widget_img_pr img {
                object-fit: contain;
                max-height: 120px;
              }
              .sr4-frm-search__content .sr4-row.sr4-widget__pr {
                  --ts-gutter-x: 20px;flex-wrap: nowrap;
              }
              .sr4-frm-search__content .sr4-widget__pr .sr4-widget__pr-title {
                  font-weight: 500;
                  line-height: 1.25;
                  font-size: 14px;
                  color: var(--secondary-color);
              }
              .sr4-frm-search__content .sr4-widget__pr-price {
                font-size: 14px;
                color: var(--secondary-price-color);
              }
              .sr4-frm-search__content .sr4-widget__pr-price ins {
                  color: var(--primary-price-color);
                  margin-left: 5px;
              }
              .rtl_true .sr4-frm-search__content .sr4-widget__pr-price ins {
                  margin-right: 5px;
                  margin-left: 0;
                  display: inline-block;
              }
              .sr4-frm-search__content .sr4-widget__pr .sr4-widget__pr-price {
                  margin-top: 1.5px;
              }
              .sr4-search-header__form-wrap .sr4-mini-search__viewAll {
                    padding: 12px 20px;
                    border-top: 1px solid rgba(var(--border-color-rgb),.35);
                    box-shadow: 0 0 10px 0 rgba(var(--border-color-rgb),.35);
             }
             .sr4-frm-search__content .sr4-widget__pr .sr4-widget__pr-title:hover,
             .sr4-search-header__form-wrap .sr4-mini-search__viewAll:hover {
                color: var(--accent-color);
             }                                                                                                                                     
            {%- endstyle -%}
            {%- liquid
             assign collection = collections[settings.search_prs_suggest]
             assign limit = 5 
             assign show_search_suggest = settings.show_search_suggest
             if shop.types.size < 3
             assign shop_types = shop.types | join: ' ' | remove: ' '
             else
             assign shop_types = 'type_the4'
             endif -%}
             
            <form data-frm-search action="{{ routes.search_url }}" method="get" class="sr4-search-header__form sr4-row sr4-g-0 sr4-align-items-center" role="search">
              <input type="hidden" name="type" value="product">
              <input type="hidden" name="options[unavailable_products]" value="{{settings.unavailable_prs}}">
              <input type="hidden" name="options[prefix]" value="last">
              {%- if settings.filter_type_search and shop_types != blank -%}
              <div data-cat-search class="sr4-search-header__type sr4-pr sr4-oh sr4-col-auto sr4-col-item">
                 <select data-name="product_type" class="sr4-truncate">
                   <option value="*">{{ 'search.general.all_categories' | t }}</option>
                   {%- for product_type in shop.types -%}{%- if product_type == blank %}{% continue-%}{% endif -%}<option value="{{product_type}}"{% if search_pr_type == product_type %} selected="selected"{% endif %}>{{product_type}}</option>{%- endfor -%}
                 </select>
                 {%- comment %}<svg class="sr4-icon-select-arrow sr4-pe-none" role="presentation" width="10" height="10" viewBox="0 0 19 12"><use xlink:href="#sr4-select-arrow"></use></svg>{% endcomment -%}
              </div>
              <div class="sr4-search-header_border sr4-col-auto sr4-col-item"></div>
             {%- endif -%}
              <div class="sr4-search-header__main sr4-pr sr4-oh sr4-d-flex sr4-col sr4-col-item">
                <input data-input-search class="sr4-search-header__input sr4-input__currentcolor" autocomplete="off" type="text" name="q" placeholder="{{ 'search.general.placeholder_products' | t }}">
                <button class="sr4-search-header__submit{% if settings.ajax_search %} sr4-pe-none{% endif %}" type="submit">
                    <span class="sr4-search-header__submit-text">{{ 'search.general.submit' | t }}</span>
                    <span class="sr4-search-header__submit-icon sr4-d-none"><svg class="sr4-icon sr4-icon--search" aria-hidden="true" focusable="false" role="presentation"><use href="#icon-h-search"></use></svg></span>
                </button>
              </div>
            </form>
            <div class="sr4-pr">
                 <div class="sr4-pa sr4-frm-search__results">
                    <div data-skeleton-search class="sr4-skeleton_wrap sr4-dn">
                       {%- for i in (1..4) -%}
                       <div class="sr4-row sr4-space-item-inner">
                          <div class="sr4-col-auto sr4-col-item sr4-widget_img_pr"><div class="sr4-skeleton_img"></div></div>
                          <div class="sr4-col sr4-col-item sr4-widget_if_pr"><div class="sr4-skeleton_txt1"></div><div class="sr4-skeleton_txt2"></div></div>
                       </div>
                       {%- endfor -%}
                    </div>
                    <div data-results-search class="sr4-frm-search__content sr4_ratioadapt sr4-current-scrollbar"{% if collection == blank or show_search_suggest == false %} style="display: none;"{% endif%}>
                      {%- if collection != blank and show_search_suggest -%}
                            {%- for product in collection.products limit:limit -%}
                            {%- render 'pr-sidebar-loop',imgatt:"",product:product,pr_url:product.url,placeholder_img:placeholder_img,price_varies_style:price_varies_style -%}
                            {%- endfor -%}
                        {%- endif -%}
                    </div>
                    {%- if collection != blank and show_search_suggest -%}
                       {%- if collection.all_products_count > limit -%}
                          <div data-viewAll-search>
                             <a href="{{collection.url}}" class="sr4-mini-search__viewAll sr4-d-block">{{ 'search.pagination.view_all' | t }} <svg width="16" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 32 32"><path d="M 18.71875 6.78125 L 17.28125 8.21875 L 24.0625 15 L 4 15 L 4 17 L 24.0625 17 L 17.28125 23.78125 L 18.71875 25.21875 L 27.21875 16.71875 L 27.90625 16 L 27.21875 15.28125 Z"/></svg></a>
                          </div>
                       {%- endif -%}
                    {%- else -%}<div data-viewAll-search></div>
                    {%- endif -%}
                 </div>
            </div>
          </div>
          <div class="sr4-col-lg-3 sr4-col-md-4 sr4-col-3 sr4-text-end sr4-col-group_btns sr4-col-item sr4-lh-1">{%- render 't4s_group_btns',se_stts:se_stts -%}</div>
      </div>
    </div>
  </div>
