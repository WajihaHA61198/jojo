{%- liquid 
    assign se_stts     = section.settings
    assign se_id       = section.id
    assign design_mode = request.design_mode
    assign loadContent = false
    if content_for_header contains '\u0026section_id=' or content_for_header contains '?section_id='
        assign loadContent = true
    endif
    assign sidebar_position = se_stts.position
    assign sidebar_size     = se_stts.size
    assign isSidebarDrawer  = se_stts.enable_drawer 
-%}  
{%- if loadContent or design_mode -%}
    <template class="sr4-d-none">
    [sr4plitlz]
        {%- if section.blocks.size > 0 -%}
            <link rel="stylesheet" href="{{ 't4s-widget.css' | asset_url }}" media="all">
            <div class="sr4-sidebar-inner">
            <div class="sr4-row sr4-row-cols-1">
                {%- for block in section.blocks -%}
                    {%- assign bk_stts = block.settings -%}
                    {%- case block.type -%}
                        {%- when 'blog_cate' -%}
                            {%- assign top_link_list = bk_stts.cat_link_list-%}
                            {%- if top_link_list == blank -%}<p class="sr4-pr-no-content">{{ 'onboarding.no_content' | t }}</p>{% continue %}{% endif -%}
                                <div class="sr4-col-item sr4-widget sr4-widget-blog-category sr4-cat-count-{{bk_stts.count}}">
                                    {%- if bk_stts.title != blank -%}
                                        <h5 class="sr4-widget-title">{{ bk_stts.title }}</h5>
                                    {%- endif -%}
                                    <ul class="sr4-blog-categories">
                                        {%- for link in linklists[top_link_list].links -%}
                                        {%- if link.type != 'blog_link' %}{% continue %}{% endif -%}
                                            <li class="sr4-cat-item sr4-pr{%- if link.active %} sr4-current-cat{%- endif -%}">
                                                <a href="{{ link.url }}">{%- render 'title_menu2',title:link.title -%} <span class="sr4-cat-count">({{ link.object.articles_count}})</a></span>
                                            </li>
                                        {%- endfor -%}
                                    </ul>
                                </div>
                            {%- when 'tags' -%}
                                {%- if blog.all_tags.size < 1 %}{% continue %}{% endif -%}
                                <div class="sr4-col-item sr4-widget sr4-widget-blog-tags sr4-tags-count-{{bk_stts.count}}">
                                    {%- if bk_stts.title != blank -%}
                                        <h5 class="sr4-widget-title">{{ bk_stts.title }}</h5>
                                    {%- endif -%}
                                    <div class="sr4-filter-tags sr4-tags-count-{{bk_stts.count}}">
                                        <ul>
                                            {%- for tag in blog.all_tags -%}
                                                {%- if current_tags contains tag -%}
                                                    <li class="sr4-is--active"><a href="{{blog.url}}">{{ tag | remove :'category_' | strip  }} <span class="sr4-blog-count">({{ tag.total_count }})</span></a></li>
                                                {%- else -%}
                                                    <li><a href="{{blog.url}}/tagged/{{ tag | handle }}">{{ tag | remove :'category_' | strip }} <span class="sr4-blog-count">({{ tag.total_count }})</span></a></li>
                                                {%- endif -%}
                                            {%- endfor -%}
                                        </ul>
                                    </div>
                                </div>
                        {%- when 'gallery' -%}
                            {%- liquid
                                assign open_link = bk_stts.open_link
                                assign ARRhtml1 = 'a,,' | split:','
                                assign ARRhtml2 = 'div,data-' | split:',' -%}
                            <div id="sr4-sidebar-{{block.id}}" class="sr4-col-item sr4-widget sr4-widget-gallery">
                                {%- if bk_stts.title != blank -%}
                                    <h5 class="sr4-widget-title">{{ bk_stts.title }}</h5>
                                {%- endif -%}  
                                <div class="sr4-row sr4_ratio1_1 sr4_position_8 sr4_cover sr4-row-cols-{{bk_stts.col_dk}} sr4-row-cols-md-{{bk_stts.col_dk}} sr4-row-cols-lg-{{bk_stts.col_dk}} sr4-gx-{{bk_stts.space_item}} sr4-gy-{{bk_stts.space_item}}">
                                {%- for i in (1..9) -%}
                                    {%- liquid
                                        assign image_list = 'img' | append:i 
                                        assign image_url_list = 'link' | append:i 
                                        assign url = bk_stts[image_url_list]
                                        assign image = bk_stts[image_list] 
                                        if url == blank 
                                            assign ARRhtml = ARRhtml2
                                        else 
                                            assign ARRhtml = ARRhtml1  
                                        endif  
                                    -%}                                               
                                    {%- if image != blank -%}
                                        <div class="sr4-col-item sr4-gallery-item">
                                            <{{ARRhtml[0]}} {{ARRhtml[1]}}href="{{url}}" {{ARRhtml[2]}}target="{{bk_stts.open_link}}" class="sr4-effect sr4-pr sr4-oh sr4-d-block">
                                                <div class="sr4_ratio" style="background: url({{ image | image_url: width: 1 }});">
                                                    <img class="lazyloadsr4" data-src="{{ image | image_url: width: 1 }}" data-widths="[100,200,400,600,700,800,900,1000,1200,1400,1600]" data-optimumx="2" data-sizes="auto" src="{% render 'img_svg',w:image.width,h:image.height %}" width="{{ image.width }}" height="{{ image.height }}" alt="{{ image.alt | escape }}">
                                                </div>
                                            </{{ARRhtml[0]}}>
                                        </div>  
                                    {%- endif -%}                                            
                                {%- endfor -%}
                                </div>
                                {%- style -%}
                                    #sr4-sidebar-{{block.id}} .sr4-gallery-item .sr4_ratio{border-radius:{{bk_stts.round}}%} 
                                {%- endstyle -%}
                            </div>
                        {%- when 'instagram' -%}
                            {%- liquid
                                assign limit = bk_stts.limit
                                if bk_stts.mode == '1' 
                                    assign acc = bk_stts.acc |default:settings.global_acc
                                else
                                    assign acc = 'ins_19041994'
                                endif -%}
        
                            <div id="sr4-sidebar-{{block.id}}" class="sr4-col-item sr4-widget sr4-widget-instagram">
                                {%- if bk_stts.title != blank -%}<h5 class="sr4-widget-title">{{ bk_stts.title }}</h5>{%- endif -%}
                                <div class="sr4-row sr4_ratio1_1 sr4_position_8 sr4_cover sr4-row-cols-{{bk_stts.col_dk}} sr4-row-cols-md-{{bk_stts.col_dk}} sr4-row-cols-lg-{{bk_stts.col_dk}} sr4-gx-{{bk_stts.space_item}} sr4-gy-{{bk_stts.space_item}}" data-inssr4-options='{ "id":"{{bk_stts.col_dk}}{{bk_stts.col_tb}}{{bk_stts.col_mb}}{{bk_stts.limit}}{{bk_stts.open_link}}","limit":{{limit}}, "acc": {{acc | base64_encode | json}}, "target":"{{bk_stts.open_link}}" }'><div class="sr4-loading--bg"></div></div>
                                <template class="sr4-icons-ins-svg"><svg class="sr4-svg-ins-image" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" width="36" height="36" fill="none" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"><path d="M22,15.5,18.55,11a2,2,0,0,0-3.14,0L12,15.27a2,2,0,0,1-3,.18l-1.1-1.07a2,2,0,0,0-2.81,0L2,17.5V20a2,2,0,0,0,2,2H20a2,2,0,0,0,2-2Z"></path><rect x="2" y="2" width="20" height="20" rx="2"></rect><line x1="6.99" y1="7" x2="7" y2="7" stroke-linecap="round" stroke-width="2.5"></line></svg>[sr4plit]<svg class="sr4-svg-ins-video" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" width="36" height="36" fill="none" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"><rect x="2" y="5" width="14" height="14" rx="2"></rect><path d="M16,10l4.55-2.28a1,1,0,0,1,1.45.9v6.76a1,1,0,0,1-1.45.9L16,14" stroke-linecap="round"></path></svg>[sr4plit]<svg class="sr4-svg-ins-album" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" width="36" height="36" fill="none" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"><rect x="2" y="2" width="14" height="14" rx="2"></rect><path d="M20,22H10a2,2,0,0,1-2-2V16h6a2,2,0,0,0,2-2V8h4a2,2,0,0,1,2,2V20A2,2,0,0,1,20,22Z"></path></svg></template>
                                {%- style -%}
                                    #sr4-sidebar-{{block.id}} .sr4-col-ins a{border-radius:{{bk_stts.round}}%} 
                                {%- endstyle -%}
                            </div>   
                        {%- when 'collection' -%}
                            {{ 'collection-products.css' | asset_url | stylesheet_tag }}
                            {%- liquid
                                if settings.within_cat and collection
                                    assign isHasCollection = true
                                else 
                                    assign isHasCollection = false
                                endif
                                    assign placeholder_img = settings.placeholder_img
                                    assign price_varies_style = settings.price_varies_style
                                if bk_stts.image_ratio == "sr4_ratioadapt"
                                    assign imgatt = ''
                                else 
                                    assign imgatt = 'data-'
                                endif -%}
                            <div id="sr4-sidebar-{{block.id}}" class="sr4-col-item sr4-widget sr4-sidebar-product-feature">
                                {%- if bk_stts.title != blank -%}
                                    <h5 class="sr4-widget-title">{{ bk_stts.title }}</h5>
                                {%- endif -%}
                                {%- if bk_stts.collection == blank -%}
                                    <p class="sr4-pr-no-content">{{ 'onboarding.no_content' | t }}</p>
                                {%- else -%}{%- assign ck_hide_all = false -%}
                                <div class="product_list_widget {{bk_stts.image_ratio}} {{bk_stts.image_position}} {{bk_stts.image_size}}">
                                    {%- for product in collections[bk_stts.collection].products limit:bk_stts.limit_pr -%}
                                    {% if isHasCollection %}{%-assign pr_url = product.url | within:collection %}{% else %}{%-assign pr_url = product.url %}{% endif -%}
                                        {%- render 'pr-sidebar-loop',imgatt:imgatt,product:product,pr_url:pr_url,placeholder_img:placeholder_img,price_varies_style:price_varies_style -%} 
                                    {%- endfor -%}
                                </div>
                                {%- endif -%}
                            </div>
                        {%- when 'post' -%}
                            {%- liquid if bk_stts.image_ratio == "sr4_ratioadapt"
                                assign imgatt = ''
                            else 
                                assign imgatt = 'data-'
                            endif -%} 
                            <div id="sr4-sidebar-{{block.id}}" class="sr4-col-item sr4-widget sr4-sidebar-post">
                                {%- if bk_stts.title!= blank -%}
                                    <h5 class="sr4-widget-title">{{ bk_stts.title }}</h5>
                                {%- endif -%}
                                {%- if bk_stts.blog == blank -%}
                                    <p>{{ 'onboarding.no_content' | t }}</p>
                                {%- else -%} 
                                    <div class="sr4-sidebar-post-list">
                                        {%- for article in blogs[bk_stts.blog].articles limit: bk_stts.limit_post -%}
                                            {%- render 'ar-sidebar-loop',article:article,date:bk_stts.date,imgatt:imgatt,image_ratio:bk_stts.image_ratio,image_position:bk_stts.image_position,image_size:bk_stts.image_size-%}
                                        {%- endfor -%}
                                    </div>
                                {%- endif -%}
                            </div> 
                        {%- when 'shipping' -%}
                            <div id="sr4-sidebar-{{block.id}}" class="sr4-col-item sr4-widget sr4-sidebar-shipping">
                                {%- if bk_stts.title != blank -%}
                                    <h5 class="sr4-widget-title">{{ bk_stts.title}}</h5>
                                {%- endif -%}
                                {%- for i in (1..6) -%}
                                    {%- assign shipping = 'shipping_' | append:i -%}
                                    {%- assign shipping_id = bk_stts[shipping] -%}
                                    {%- if shipping_id != blank -%}
                                        {%- assign shipping = shipping_id | replace: ' ,', ',' | replace: ', ', ',' | split: ',' -%}
                                        <div class="sr4-row sr4-gx-0 sr4-gy-0 sr4-text-left sr4-space-item-inner">
                                            {%- if shipping[0] != blank -%}
                                                <div class="sr4-sidebar-shipping-icon sr4-col-item icon sr4-col-auto">
                                                    <i class="{{shipping[0]}}"></i>
                                                </div>
                                            {%- endif -%}
                                            <div class="sr4-col-item sr4-col">
                                                <h4 class="sr4-sidebar-shipping-title">{{shipping[1]}}</h4>
                                                <p class="sr4-sidebar-shipping-desc">{{shipping[2]}}</p>
                                            </div>
                                        </div>
                                    {%- endif -%}
                                {%- endfor -%}
                            </div>
                        {%- when 'html' -%}
                            <div id="sr4-sidebar-{{block.id}}" class="sr4-col-item sr4-widget sr4-rte sr4-sidebar-content-page">
                                {%- if bk_stts.title != blank -%}
                                    <h5 class="sr4-widget-title">{{ bk_stts.title }}</h5>
                                {%- endif -%}
                                {%- if bk_stts.custom_html != blank -%}{{ bk_stts.custom_html }}{%- endif -%}
                            </div>
                        {%- when 'text' -%}
                            <div id="sr4-sidebar-{{block.id}}" class="sr4-col-item sr4-widget sr4-sidebar-content-page sr4-rte">
                                {%- if bk_stts.title != blank -%}
                                    <h5 class="sr4-widget-title">{{ bk_stts.title }}</h5>
                                {%- endif -%}
                                {%- if bk_stts.text_html != blank -%}{{bk_stts.text_html}}{%- endif -%}
                            </div>
                        {%- when 'image' -%}
                            <div id="sr4-sidebar-{{block.id}}" class="sr4-col-item sr4-widget sr4-sidebar-image">
                                {%- assign image = bk_stts.image_sidebar -%}
                                {%- if image -%}
                                    {{ 'custom-effect.css' | asset_url | stylesheet_tag }}
                                    <div class="sr4-image sr4-pr sr4-eff-img-{{bk_stts.img_effect}} sr4-countdown-pos-{{bk_stts.countdown_pos}} sr4-text-{{bk_stts.content_align}} id="b_{{block.id}}">
                                        {%- if bk_stts.b_link != blank -%}
                                        <a class="sr4-oh sr4-d-block" href="{{ bk_stts.b_link }}" target="{{bk_stts.open_link}}">
                                        {%- endif -%}
                                            <img class="lazyloadsr4 sr4-obj-eff" data-src="{{ image | image_url: width: 1 }}" data-widths="[100,200,400,600,700,800,900,1000,1200,1400,1600]" data-optimumx="2" data-sizes="auto" src="{% render 'img_svg',w:image.width,h:image.height %}" width="{{ image.width }}" height="{{ image.height }}" alt="{{ image.alt | escape }}">
                                            {%- if bk_stts.countdown_pos == "2"  -%}<span class="lazyloadsr4-loader"></span>{%- endif -%}
                                        {%- if bk_stts.b_link != blank -%}
                                        </a>
                                        {%- endif -%}
                                        {%- if bk_stts.date != blank -%}
                                            {{ 'countdown.css' | asset_url | stylesheet_tag }}
                                            <div class="sr4-sidebar-countdown sr4-countdown sr4-countdown-des-{{bk_stts.cdt_des}} sr4-countdown-size-{{bk_stts.cdt_size}}" id="b_{{block.id}}" {%- render 'bk_cus_style',type:'countdown',bk_stts:bk_stts,ani_delay:ani_delay -%}>
                                                <div class="time" data-countdown-sr4 data-date='{{bk_stts.date}}'>
                                                <span class="countdown-days">
                                                    <span class="cd_timet4 cd-number">%-D</span>
                                                    <span class="cd_txtt4 cd-text">%!D:{{"sections.countdown_text.day" | t}},{{"sections.countdown_text.day_plural" | t}};</span>
                                                </span>
                                                <span class="countdown-hours"> 
                                                    <span class="cd_timet4 cd-number">%H</span> 
                                                    <span class="cd_txtt4 cd-text">%!H:{{"sections.countdown_text.hr" | t}},{{"sections.countdown_text.hr_plural" | t}};</span>
                                                </span>
                                                <span class="countdown-min">
                                                    <span class="cd_timet4 cd-number">%M</span> 
                                                    <span class="cd_txtt4 cd-text">%!M:{{"sections.countdown_text.min" | t}},{{"sections.countdown_text.min_plural" | t}};</span>
                                                </span>
                                                <span class="countdown-sec">
                                                    <span class="cd_timet4 cd-number">%S</span> 
                                                    <span class="cd_txtt4 cd-text">%!S:{{"sections.countdown_text.sec" | t}},{{"sections.countdown_text.sec_plural" | t}};</span>
                                                </span>
                                                </div>
                                            </div>
                                        {%- endif -%} 
                                    </div>
                                {%- endif -%}
                            </div>
                        {%- when 'cus_socials' -%}
                            {{ 'icon-social.css' | asset_url | stylesheet_tag }}
                            <div id="sr4-sidebar-{{block.id}}" class="sr4-col-item sr4-widget sr4-widget-socials">
                                {%- if bk_stts.heading != blank -%}
                                  <h5 class="sr4-widget-title">{{ bk_stts.heading }}</h5>
                                {%- endif -%} 
                                <div class="sr4-socials-block sr4-setts-color-{{bk_stts.use_color_set}} social-{{block.id}}" style="--cl:{{bk_stts.icon_cl}};--bg-cl:{{bk_stts.bg_cl}};--mgb: {{bk_stts.mgb}}px;--mgb-mb: {{bk_stts.mgb_mb}}px; --bd-radius:{{bk_stts.bd_radius}}px;">
                                   {%- if bk_stts.social_mode == '1' -%} 
                                     {%- assign follow_social = true -%} 
                                   {%- else -%} 
                                     {%- assign share_image = settings.share_image | default: page_image | default: settings.logo -%} 
                                   {%- endif -%} 
                                    {%- render 'social_sharing',style:bk_stts.social_style,use_color_set:bk_stts.use_color_set,size:bk_stts.social_size,space_h_item:bk_stts.space_h_item,space_h_item_mb:bk_stts.space_h_item_mb,space_v_item:bk_stts.space_v_item,space_v_item_mb:bk_stts.space_v_item_mb,share_permalink:shop.url,share_title:shop.name,share_image:share_image,follow_social:follow_social -%} 
                                </div>
                            </div>
                    {%- endcase -%}  
                {%- endfor -%}
            </div>
        </div>
        {%- else -%}
            <span class="sr4-onboarding-info">{{'onboarding.no_content'|t}}</span>
    {%- endif -%}
    [sr4plitlz]
        <link rel="stylesheet" href="{{ 'drawer.min.css' | asset_url }}" media="all">
        <div id="drawer-{{se_id}}" class="sr4-drawer sr4-drawer__left" aria-hidden="true">
            <div class="sr4-drawer__header"><span>{{'general.sidebar.title' | t}}</span><button class="sr4-drawer__close" data-drawer-close aria-label="{{'general.sidebar.close' | t}}"><svg class="sr4-iconsvg-close" role="presentation" viewBox="0 0 16 14"><path d="M15 0L1 14m14 0L1 0" stroke="currentColor" fill="none" fill-rule="evenodd"></path></svg></button></div>
            <div class="sr4-drawer__content">
                <div class="sr4-drawer__main sr4-current-scrollbar">[sr4plitlz2]</div>
                <div class="sr4-drawer__footer"></div>
            </div>
        </div>     
    [sr4plitlz]
    </template>
{%- endif -%}
<button data-sidebar-id='{{se_id}}' data-sidebar-{{isSidebarDrawer}} data-drawer-options='{ "id":"#drawer-{{se_id}}" }' class="sr4-btn-sidebar"><span class="sr4-btn-sidebar-icon"><svg viewBox="0 0 24 24" width="22" height="22" stroke="currentColor" stroke-width="1.5" fill="none" stroke-linecap="round" stroke-linejoin="round" class="css-i6dzq1"><rect x="3" y="3" width="18" height="18" rx="2" ry="2"></rect><line x1="9" y1="3" x2="9" y2="21"></line></svg></span> <span class="sr4-btn-sidebar-text">{{'general.sidebar.open' | t}}</span></button>
<style>
    button.sr4-btn-sidebar {
        position: fixed;
        top: 50%;
        z-index: 100;
        background-color: var(--sr4-light-color);
        color:  var(--sr4-dark-color);
        box-shadow: 0 0 3px rgb(0 0 0 / 15%);
        transition: all .6s cubic-bezier(.19,1,.22,1);
        display: flex;
        align-items: center;
        flex-direction: row;
        flex-wrap: nowrap;
        overflow: hidden;
        min-width: 50px;
        height: 50px;
        padding: 0;
    }
    .sr4-btn-sidebar .sr4-btn-sidebar-icon {
        width: 50px;
        height: 50px;
        display: inline-flex;
        justify-content: center;
        align-items: center;
    }
    .sr4-btn-sidebar .sr4-btn-sidebar-text {
        padding: 0;
        max-width: 0;
        white-space: nowrap;
        overflow: hidden;
        text-overflow: ellipsis;
        font-weight: 500;
        will-change: padding, max-width;
        transition: padding 0.4s cubic-bezier(.175,.885,.32,1.15),max-width 0.4s cubic-bezier(.175,.885,.32,1.15);
    }
    .sr4-btn-sidebar:hover .sr4-btn-sidebar-text {
        padding-right: 25px;
        max-width: 280px;
    }
    .sr4-sidebar-inner .sr4-widget:not(:last-child){margin-bottom: {{se_stts.space}}px; }
    #drawer-{{se_id}} .sr4-drawer__main{overflow-x: hidden;padding:20px;}
    {%- unless isSidebarDrawer %}
    @media screen and (min-width: 1025px) {
        .sr4-sidebar { display: block }
        button.sr4-btn-sidebar,.sr4-section-sidebar{display: none;}
        {%- if sidebar_size == 'small' -%}
        .sr4-main-area {flex: 0 0 auto;width: 83.33333333%; }.sr4-sidebar {flex: 0 0 auto;width: 16.66666667%; }
        {%- elsif sidebar_size == 'medium' -%}
        .sr4-main-area {flex: 0 0 auto;width: 75%; }
        {%- else -%}
        .sr4-main-area {flex: 0 0 auto;width: 66.66666667%; }.sr4-sidebar {flex: 0 0 auto;width: 33.33333333%; }
        {%- endif -%}
    }
    .sr4-section-sidebar ~ .sr4-section-main .sr4-main-area{order: 10}
    .sr4-section-sidebar ~ .sr4-section-main .sr4-sidebar{order: 1}
    {% endunless -%}
    {% if se_stts.br_style != 'none' and isSidebarDrawer == false -%}
    /* css border conffig */
    .sr4-sidebar-inner {
        border: 1px {{se_stts.br_style}} {{se_stts.brcolor}};
        padding: {{se_stts.sidebar_pd}}px;
        border-radius: {{se_stts.sidebar_bdr}}px;
    }
    {%- endif -%}
    {%- if design_mode -%}
        .sr4-section-sidebar {position: fixed;top: 100px;z-index: 20;}
    {%- endif -%}
</style>
{%- schema -%}
{
    "name": "Sidebar blog",
    "class": "sr4-section sr4-section-sidebar sr4_tp_ins sr4_tp_cd",
    "settings": [
        {
            "type": "checkbox",
            "id": "enable_drawer",
            "label": "Enable layout drawer",
            "default": true
        },
        {
            "type": "range",
            "id": "space",
            "label": "Space bottom",
            "info": "Space bottom between block",
            "default": 50,
            "min": 20,
            "max": 100,
            "step": 1,
            "unit": "px"
        },
        {
            "type": "paragraph",
            "content": "+ Configs only working when disable layout drawer"
        },
        {
            "type": "select",
            "id": "size",
                "options": [
                {
                    "value": "small",
                    "label": "Small"
