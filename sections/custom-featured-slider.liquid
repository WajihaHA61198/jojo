                      data-src="{{ image | image_url: width: 1 }}"
                      data-widths="[800, 1000, 1200, 1400, 1600, 1800, 2000, 2200, 2500, 3000, 3400, 3800, 4100]"
                      data-optimumx="2"
                      data-sizes="auto"
                      src="{% render 'img_svg',w:image.width,h:image.height %}"
                      width="{{ image.width }}"
                      height="{{ image.height }}"
                      alt="{{ image.alt | escape }}">
