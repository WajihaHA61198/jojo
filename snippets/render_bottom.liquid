<template id="btns_pr_temp">
  {%- if enable_quickview -%}
    <a
      href="#sr4_pr_url"
      data-tooltip=""
      data-id="id_nt_94"
      rel="nofollow"
      class="sr4-pr-item-btn sr4-pr-quickview"
      data-action-quickview
      ><span class="sr4-svg-pr-icon">
        <svg viewBox="0 0 24 24">
          <use xlink:href="#sr4-icon-qv"></use>
        </svg></span
      ><span class="sr4-text-pr">{{ 'products.product_card.quick_view' | t }}</span></a
    >
  {%- endif -%}
  [split_t4nt]

  {%- if settings.enable_compe -%}
    <a
      href="#sr4_pr_url"
      data-tooltip=""
      data-id="id_nt_94"
      data-handle="handle_nt_94"
      rel="nofollow"
      class="sr4-pr-item-btn sr4-pr-compare"
      data-action-compare
      ><span class="sr4-svg-pr-icon">
        <svg class="sr4-svg-cp" viewBox="0 0 24 24">
          <use xlink:href="#sr4-icon-cp"></use>
        </svg></span
      ><span class="sr4-text-pr">{{ 'products.product.compare' | t }}</span></a
    >
  {%- endif -%}
  [split_t4nt]

  {%- if settings.wishlist_mode == '1' -%}
    <a
      href="#sr4_pr_url"
      data-tooltip=""
      data-id="id_nt_94"
      rel="nofollow"
      class="sr4-pr-item-btn sr4-pr-wishlist"
      data-action-wishlist
      ><span class="sr4-svg-pr-icon">
        <svg viewBox="0 0 24 24">
          <use xlink:href="#sr4-icon-wis"></use>
        </svg></span
      ><span class="sr4-text-pr">{{ 'products.product.add_to_wishlist' | t }}</span></a
    >
  {%- elsif settings.wishlist_mode == '2' and shop.customer_accounts_enabled -%}
    {%- if customer -%}
      <a
        href="#sr4_pr_url"
        data-tooltip=""
        data-id="id_nt_94"
        data-handle="handle_nt_94"
        rel="nofollow"
        class="sr4-pr-item-btn sr4-pr-wishlist"
        data-action-wishlist
        ><span class="sr4-svg-pr-icon">
          <svg viewBox="0 0 24 24">
            <use xlink:href="#sr4-icon-wis"></use>
          </svg></span
        ><span class="sr4-text-pr">{{ 'products.product.add_to_wishlist' | t }}</span></a
      >
    {%- else -%}
      <a
        href="{{routes.account_login_url}}"
        data-tooltip=""
        data-id="id_nt_94"
        rel="nofollow"
        class="sr4-pr-item-btn sr4-pr-wishlist"
        data-action-wishlist-login
        ><span class="sr4-svg-pr-icon">
          <svg viewBox="0 0 24 24">
            <use xlink:href="#sr4-icon-wis"></use>
          </svg></span
        ><span class="sr4-text-pr">{{ 'products.product.login_to_wishlist' | t }}</span></a
      >
    {%- endif -%}
